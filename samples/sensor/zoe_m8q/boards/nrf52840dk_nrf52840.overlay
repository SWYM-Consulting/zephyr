# FIXME - Should't need SPI configuration

&spi0 {
	compatible = "nordic,nrf-spi";
	status = "okay";
	sck-pin = <27>;
	mosi-pin = <26>;
	miso-pin = <23>;
	cs-gpios = <&gpio0 22 GPIO_ACTIVE_LOW>;
	zoe_m8q@0 {
		compatible = "u_blox,zoe-m8q";
		spi-max-frequency = <24000000>;
		reg = <0>;
		label = "ZOE-M8Q";
		int-gpios = <&gpio0 18 GPIO_ACTIVE_HIGH>;
	};
};
